				}
				pTemp[playerid][pTaxiStart] = VehicleInfo[vehicleid][vMillage];
				pTemp[playerid][pTaxiPass] = passid;
				pTemp[playerid][pTaxiGoing] = true;
				pTemp[playerid][pTaxiTurn][0] = INVALID_PLAYER_ID;
