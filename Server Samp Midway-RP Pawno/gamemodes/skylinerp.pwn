	    switch(word[i])
	    {
	        case 'A': word[i] = 'a';
	        case 'B': word[i] = 'b';
	        case 'C': word[i] = 's';
	        case 'D': word[i] = 'd';
	        case 'E': word[i] = 'e';
	        case 'F': word[i] = 'f';
	        case 'G': word[i] = 'g';
