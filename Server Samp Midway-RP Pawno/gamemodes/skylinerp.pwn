	if(GetPlayerDistanceToPlayer(playerid,params[0]) > 20.0 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы далеко друг от друга");
	if(PI[playerid][pMoney] < params[1]) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}У вас недостаточно денег");
	if(PI[params[0]][pMoney] < params[1]) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}У соперника недостаточно денег");

//проверка на скин
	if(GetPlayerSkin(playerid) == 80 && GetPlayerSkin(params[0]) == 80) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы не можете вызвать на бой соперника в одинаковой экипировке, переоденьтесь");
    if(GetPlayerSkin(playerid) == 81 && GetPlayerSkin(params[0]) == 81) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы не можете вызвать на бой соперника в одинаковой экипировке, переоденьтесь");
    if((GetPlayerSkin(playerid) == 80  && GetPlayerSkin(params[0]) == 81) || (GetPlayerSkin(playerid) == 81  && GetPlayerSkin(params[0]) == 80))
	{
	    box_money_box = params[1];
		if(box_money_box < 10000 || box_money_box > 1000000) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Сумма не может быть меньше {33AA33}10000$ {FFFFFF}или больше {33AA33}1000000$");

		box_battle = true; //Занимаем ринг

	    box_one = playerid;
	    box_two = params[0];


		box_proverka[params[0]] = 1;
		box_proverka[playerid] = 1;


		new boxstring[128];

		format(boxstring, sizeof boxstring, "{1E90FF}%s {FFFFFF}предлагает провести бой за {33AA33}%d$", PN(playerid), params[1]);
		SendClientMessage(params[0], COLOR_WHITE, boxstring);
		SendClientMessage(params[0], COLOR_WHITE, "Чтобы подтвердить нажмите {33AA33}Y{FFFFFF} иначе {FF0000}N");

		format(boxstring, sizeof boxstring, "Вы предложили {1E90FF}%s {269BD8}провести бой за {33AA33}%d$", PN(params[0]), params[1]);
		SendClientMessage(playerid, 0x269BD8FF, boxstring);


		temp_box_sender[params[0]] = playerid + 1;
		temp_box_receiver[playerid] = params[0] + 1;
	    box_money[playerid] = params[1];
	}
	else
	{
	    return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Нельзя выступать на ринге не переодевшись в спортивную экипировку");
	}
	
    return true;
}

CMD:disbattle(playerid)
{
	if(box_battle == true) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы не можете отказаться от боя или предложения пока ринг занят");
	box_proverka[playerid] = 0;
	temp_box_receiver[playerid] -= 1;
	box_proverka[temp_box_receiver[playerid]] = 0;
	temp_box_receiver[temp_box_receiver[playerid]] = INVALID_PLAYER_ID;
    temp_box_receiver[playerid] = INVALID_PLAYER_ID;
    box_money[playerid] = 0;
    box_battle = false;
    box_one = -1;
    box_two = -1;
    box_money_box = 0;
    return true;
}
