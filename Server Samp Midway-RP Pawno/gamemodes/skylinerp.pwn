//метка админ сменить пароль
CMD:asetpass(playerid, params[])
{
    if(PI[playerid][pAdmin] < 1)
		return 1;

    if(!IsPlayerLogged[playerid])
		return false;

    if(sscanf(params, "dd", params[0], params[1]))
	{
	    SendClientMessage(playerid, COLOR_WHITE, !"{FFC800}[Подсказка] {FFFFFF}Пароль должен состоять только из чисел");
		return SendClientMessage(playerid, COLOR_WHITE, !"{FFC800}[Подсказка] {FFFFFF}/asetpass [старый пароль] [новый пароль]");
	}
    if(strlen(params[0]) > 16)
		return SendClientMessage(playerid, COLOR_WHITE, "{FFC800}[Подсказка] {FFFFFF}Пароль должен быть не больше 16 чисел");

	if(strlen(params[1]) > 16)
		return SendClientMessage(playerid, COLOR_WHITE, "{FFC800}[Подсказка] {FFFFFF}Пароль должен быть не больше 16 чисел");

	if(params[0] != PI[playerid][pAdminPassword])
	    return SendClientMessage(playerid, COLOR_GREEN, !"{FF2222}[Ошибка] {FFFFFF}Пароль администратора не совпадает");

    
	UpdatePlayerData(playerid, "AdminPassword", params[1]);
	PI[playerid][pAdminPassword] = params[1];
    SendClientMessage(playerid, COLOR_GREEN, !"[Успех] {FFFFFF}Вы успешно сменили пароль администратора, запишите его что бы не потерять");
    
    return 1;
}
