stock UpdateBiz(bizid,id)
{
    new temp[450];
    if(BizInfo[bizid][bLock])
    {
        new mafiastr[256], nickname[MAX_PLAYER_NAME];
        switch(BizInfo[bizid][bKrasher])
        {
            case 12: format(mafiastr,sizeof(mafiastr),"Украинская мафия");
            case 13: format(mafiastr,sizeof(mafiastr),"Русская мафия");
            case 14: format(mafiastr,sizeof(mafiastr),"Итальянская мафия");
            default: format(mafiastr,sizeof(mafiastr),"-");
        }

        if(GetString(BizInfo[bizid][bOwner],"The State"))
            strcat(nickname, "нет");

        else
            strcat(nickname, BizInfo[bizid][bOwner]);

        if(BizInfo[bizid][bEnterPrice])
            format(temp, sizeof temp, "{33AA33}%s\n{4682B4}Владелец: {FFFFFF}%s\n{4682B4}Стоимость: {FFFFFF}$%d\n{4682B4}Контроль: {FFFFFF}%s\n{4682B4}Цена за вход: {ffffff}%d", BizInfo[bizid][bName], nickname, BizInfo[bizid][bCost], mafiastr, BizInfo[bizid][bEnterPrice]);

        else
            format(temp, sizeof temp, "{33AA33}%s\n{4682B4}Владелец: {FFFFFF}%s\n{4682B4}Стоимость: {FFFFFF}$%d\n{4682B4}Контроль: {FFFFFF}%s", BizInfo[bizid][bName], nickname, BizInfo[bizid][bCost], mafiastr);
    }
    else strcat(temp, "\t\t\t{FF0000}ЗАКРЫТО\n{AFAFAF}(Информация недоступна)");

    UpdateDynamic3DTextLabelText(BusinessText[bizid],COLOR_WHITE,temp);

    if(id == 1 ) SaveBiz(bizid);
    return true;
}
