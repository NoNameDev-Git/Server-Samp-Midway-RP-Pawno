	new str[256], srrt[512], total = 0;
	for(new i = 0; i < 9; i++)
	{
		if(strcmp(PickName[i], "None", false))
		{
			format(str, sizeof(str), "{ffffff}Кандидат #%i - {7AC71F}%s {ffffff}(Голосов: {FFC800}%d{ffffff})\n", i+1, PickName[i], PickVote[i]);
			strcat(srrt, str);
