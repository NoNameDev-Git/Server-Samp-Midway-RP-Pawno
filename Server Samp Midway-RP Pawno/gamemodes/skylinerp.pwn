//метка семья купить семейное авто
stock BuyFamilyCar(playerid, vehmodelid, price)
{
    if(!IsPlayerLogged[playerid])
		return 1;

	if(PI[playerid][pMoney] < price)
	    return SendClientMessage(playerid, -1, "{FF2222}[Ошибка]{FFFFFF}У вас недостаточно денег для покупки транспорта");


	new countfcar = 0;
    for(new i; i < OWNABLECARSF; i++)
	{
	    if(CarInfof[i][cFamID] == PI[playerid][pFamily])
	    {
	        countfcar ++;
	        if(countfcar >= gFamily[PI[playerid][pFamily]-1][fCars])
	            return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы не можете приобрести транспорт, все слоты заняты");
		}
	}
	countfcar ++;

	new fcFamID, fcNumID, fcModelID, fcColor_1, fcColor_2, fcSignal;
	new Float:fcFuel, Float:fcHealth, Float:fcMilage;
	new Float:fcPark_X, Float:fcPark_Y, Float:fcPark_Z, Float:fcPark_R;
	
	
	fcFamID = PI[playerid][pFamily];
    fcNumID = countfcar;
    fcModelID = vehmodelid;
    fcColor_1 = 1;
    fcColor_2 = 0;
    fcFuel = 100;
    fcHealth = 1000.0;
    fcMilage = 0;

    new year, month, day;
	getdate(year, month, day);
	new fcDate[20];
	format(fcDate, sizeof fcDate, "%02d.%02d.%02d", day, month, year);
	
    fcSignal = 0;
    fcPark_X = 0;
    fcPark_Y = 0;
    fcPark_Z = 0;
    fcPark_R = 0;

    

	new request[2513];
	format(request, sizeof(request), "INSERT INTO `familycars` (`cFam`, `cNum`, `cModel`, `cColor1`, `cColor2`, `cNumber`, `cFuel`,`cHealth`, `cMilage`, `cDate`, `cSignal`,`cPark_X`, `cPark_Y`,`cPark_Z`,`cPark_R`) VALUES ('%d', '%d', '%d', '%d', '%d', '%s', '%f', '%f', '%f', '%s', '%d', '%f', '%f','%f','%f')",
    fcFamID, fcNumID, fcModelID, fcColor_1, fcColor_2,
    gFamily[PI[playerid][pFamily]-1][fName], fcFuel, fcHealth, fcMilage,
    fcDate, fcSignal, fcPark_X, fcPark_Y, fcPark_Z, fcPark_R);
	mysql_query(mysql, request);

    LoadFamilyCarBuy();

	GiveMoney(playerid, -price, "покупка авто семья");
	SendClientMessage(playerid, COLOR_WHITE, !"{7AC71F}[Успешно] {FFFFFF}Вы приобрели семейный транспорт");
    SendClientMessage(playerid, COLOR_WHITE, !"{FFC800}[Подсказка] {FFFFFF}Установить координаты спавна семейного т/с введите /fsetpos");
    return true;
}
forward LoadFamilyCarBuy();
public LoadFamilyCarBuy()
{
	new Cache:result = mysql_query(mysql, "SELECT * FROM `familycars`");
	OWNABLECARSF = cache_get_row_count(mysql);

	for(new idx; idx < OWNABLECARSF; idx++)
	{
	    CarInfof[idx][cID] = cache_get_row_int(idx, 0, mysql);
		CarInfof[idx][cFamID] = cache_get_row_int(idx, 1, mysql);
		CarInfof[idx][cNumID] = cache_get_row_int(idx, 2, mysql);
		CarInfof[idx][cModelID] = cache_get_row_int(idx, 3, mysql);
		CarInfof[idx][cColor_1] = cache_get_row_int(idx, 4, mysql);
		CarInfof[idx][cColor_2] = cache_get_row_int(idx, 5, mysql);
		cache_get_row(idx, 6, CarInfof[idx][cNumber], mysql, 12);
		CarInfof[idx][cFuel] = cache_get_row_float(idx, 7, mysql);
		CarInfof[idx][cHealth] = cache_get_row_float(idx, 8, mysql);
		CarInfof[idx][cMilage] = cache_get_row_float(idx, 9, mysql);
		cache_get_row(idx, 10, CarInfof[idx][cDate], mysql, 20);

		new
		    tuning[80];

		cache_get_row(idx, 11, tuning, mysql, 100);
		sscanf(tuning, "p<|>iiiiiiiiiiiiii", CarInfof[idx][cTuning][0], CarInfof[idx][cTuning][1], CarInfof[idx][cTuning][2], CarInfof[idx][cTuning][3], CarInfof[idx][cTuning][4],
											 CarInfof[idx][cTuning][5], CarInfof[idx][cTuning][6], CarInfof[idx][cTuning][7], CarInfof[idx][cTuning][8], CarInfof[idx][cTuning][9],
											 CarInfof[idx][cTuning][10], CarInfof[idx][cTuning][11], CarInfof[idx][cTuning][12], CarInfof[idx][cTuning][13]);

        CarInfof[idx][cPaintJob] = cache_get_row_int(idx, 12, mysql);
        CarInfof[idx][cSignal] = cache_get_row_int(idx, 13, mysql);
        CarInfof[idx][cPark_X] = cache_get_row_float(idx, 14, mysql);
        CarInfof[idx][cPark_Y] = cache_get_row_float(idx, 15, mysql);
        CarInfof[idx][cPark_Z] = cache_get_row_float(idx, 16, mysql);
        CarInfof[idx][cPark_R] = cache_get_row_float(idx, 17, mysql);
        CarInfof[idx][cVehID] = cache_get_row_int(idx, 18, mysql);
        CarInfof[idx][cSlotID] = idx;
	}
	
	cache_delete(result, mysql);
	
	return true;
}
