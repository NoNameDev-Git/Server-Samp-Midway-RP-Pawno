stock CheckValidation(const text[])
{
	for(new i; i < strlen(text); i ++)
	{
		if( (text[i] >= 'a' && text[i] <= 'z') ||
		(text[i] >= 'A' && text[i] <= 'Z') ||
		(text[i] >= '0' && text[i] <= '9') ||
		(text[i] >= 'А' && text[i] <= 'Я') ||
		(text[i] >= 'а' && text[i] <= 'я') ) continue;
		else return false;
	}
	return true;
}

