//метка авторынок сток покупки авто на авторынке
stock BuyCarAutoMarket(playerid, value)
{
    if(PI[playerid][pCars] != 0) return SendClientMessage(playerid, COLOR_WHITE, !"{F70000}[Ошибка] {FFFFFF}У вас уже есть личный т/с");
	if(PI[playerid][pHouseKey] == 0) return SendClientMessage(playerid, COLOR_WHITE, !"{F70000}[Ошибка] {FFFFFF}У вас нет дома для приобретения т/с");
	if(PI[playerid][pMoney] < AutoMarketInfo[value][aCost]) return SendClientMessage(playerid, COLOR_WHITE, !"{F70000}[Ошибка] {FFFFFF}У вас недостаточно средств для приобретения т/с");
	if(AutoMarketInfo[value][aActive] == 0)
	{
	    new str[300];
	    format(str, sizeof(str),"{F70000}[Ошибка] {FFFFFF}Парковочное место №%d не занято", value+1);
		return SendClientMessage(playerid, COLOR_WHITE, str);
	}
	

	PI[playerid][pCars] = AutoMarketInfo[value][aCar_ID];
	UpdatePlayerData(playerid, "cars", AutoMarketInfo[value][aCar_ID]);

    UpdateCarData("cOwnerID", PI[playerid][pID], AutoMarketInfo[value][aCar_ID]);
    
    for(new r; r < MAX_PLAYERS; r++)
    {
		if(PI[r][pCars] == AutoMarketInfo[value][aCar_ID])
		{
            PI[r][pCars] = 0;
		}
	}
    PI[playerid][pCars] = AutoMarketInfo[value][aCar_ID];

	new request[2513];
	format(request, sizeof(request), "UPDATE `accounts` SET `Cars` = '%d' WHERE `ID` = '%d'",  0, AutoMarketInfo[value][aOwner_ID]);
	mysql_query(mysql, request);


	for(new t; t < MAX_HOUSES; t++)
    {
		if(HouseInfo[t][hID] == AutoMarketInfo[value][aHome])
		{

			HouseInfo[t][hPark_X] = AutoMarketInfo[value][ahPark_X];
            HouseInfo[t][hPark_Y] = AutoMarketInfo[value][ahPark_Y];
            HouseInfo[t][hPark_Z] = AutoMarketInfo[value][ahPark_Z];
            HouseInfo[t][hPark_R] = AutoMarketInfo[value][ahPark_R];


			format(request, sizeof(request), "UPDATE `houses` SET `Park_X` = '%f', `Park_Y` = '%f', `Park_Z` = '%f', `Park_R` = '%f' WHERE `ID` = '%d'",
			HouseInfo[t][hPark_X],
			HouseInfo[t][hPark_Y],
			HouseInfo[t][hPark_Z],
			HouseInfo[t][hPark_R],
			HouseInfo[t][hID]);
			mysql_query(mysql, request);
		}
	}

	//снятие денег
	new mes[24];
	GiveMoney(playerid, - AutoMarketInfo[value][aCost], "покупка авторынок");
	format(mes,sizeof(mes),"~r~- %d$",AutoMarketInfo[value][aCost]);
	GameTextForPlayer(playerid, mes, 3000, 1);

	  //получение денег продавцу
	InsertAutoMarketPriceData(AutoMarketInfo[value][aOwner_ID], AutoMarketInfo[value][aCost]);
	LoadAutomarketPrice();

	new formattext[300];
	format(formattext, sizeof(formattext), "{FF1493}Место №%d\n{FFFFFF}Статус: {32CD32}Свободно", value+1);
	UpdateDynamic3DTextLabelText(AutoMarketText[value], 0xFF1493FF, formattext);

	AutoMarketInfo[value][aCar_ID] = 0;
    AutoMarketInfo[value][aOwner_ID] = 0;
    AutoMarketInfo[value][aCost] = 0;
    AutoMarketInfo[value][aActive] = 0;
    AutoMarketInfo[value][ahPark_X] = 0.0;
    AutoMarketInfo[value][ahPark_Y] = 0.0;
    AutoMarketInfo[value][ahPark_Z] = 0.0;
    AutoMarketInfo[value][ahPark_R] = 0.0;
    AutoMarketInfo[value][aHome] = 0;

	format(request, sizeof(request), "UPDATE `automarket` SET `ID_Car` = '%d', `Owner_ID` = '%d', `Cost` = '%d', `Active` = '%d', `hPark_X` = '%f', `hPark_Y` = '%f', `hPark_Z` = '%f', `hPark_R` = '%f', `Home` = '%d' WHERE `ID` = '%d'",
	AutoMarketInfo[value][aCar_ID],
	AutoMarketInfo[value][aOwner_ID],
	AutoMarketInfo[value][aCost],
	AutoMarketInfo[value][aActive],
	AutoMarketInfo[value][ahPark_X],
	AutoMarketInfo[value][ahPark_Y],
	AutoMarketInfo[value][ahPark_Z],
	AutoMarketInfo[value][ahPark_R],
	AutoMarketInfo[value][aHome],
	AutoMarketInfo[value][aID]);
	mysql_query(mysql, request);
	return 1;
}



