public OnPlayerEnterRaceCheckpoint(playerid)
{
//метка автошкола вождение первый чекпоинт
	if(race_a[playerid] != -1)
	{
		race_a[playerid]++;
		new i;
        i = race_a[playerid];
		if(race_a[playerid]<sizeof(Race_auto))
		{
		    //метка автошкола проерка на скорость автомобиля
			if(GetPlayerVehicleSpeed(playerid) >= 102.0)
			{
                auto_school_car_error[playerid] += 1;
                new auto_string[76];
                format(auto_string, sizeof(auto_string), "{FF2222}[Ошибка] {FFFFFF}Вы допустили ошибку при вождении т/с {FF2222}[%d/3]",auto_school_car_error[playerid]);
                SendClientMessage(playerid, COLOR_WHITE, auto_string);
                SendClientMessage(playerid, COLOR_WHITE, !"{FF8C00}[Замечание] {FFFFFF}Старайтесь больше не привышать скорость, иначе провалите экзамен");
                if(auto_school_car_error[playerid] >= 3)
                {
					race_a[playerid] = -1;
					SetPVarInt(playerid,"auto_school_car_work", 0);
					auto_school_car_key[playerid] = false;
					auto_school_car_error[playerid] = 0;
					race_a[playerid] = -1;
					ExitCar(playerid);
					DisablePlayerRaceCheckpoint(playerid);
					return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы провалили экзамен, для того что бы пересдать вернитесь в автошколу");
				}
			}
			if (i < 19)
			{
                DisablePlayerRaceCheckpoint(playerid);
				SetPlayerRaceCheckpoint(playerid,0,Race_auto[i][0],Race_auto[i][1],Race_auto[i][2],Race_auto[i][3],Race_auto[i][4],Race_auto[i][5],4.0);
			}
			else if (i > 20 && i < 120)
			{
                DisablePlayerRaceCheckpoint(playerid);
				SetPlayerRaceCheckpoint(playerid,0,Race_auto[i][0],Race_auto[i][1],Race_auto[i][2],Race_auto[i][3],Race_auto[i][4],Race_auto[i][5],4.0);
			}
			else if (i == 19 || i == 48)
			{
			    DisablePlayerRaceCheckpoint(playerid);
				SetPlayerRaceCheckpoint(playerid,1,Race_auto[i][0],Race_auto[i][1],Race_auto[i][2],Race_auto[i][3],Race_auto[i][4],Race_auto[i][5],4.0);
			}
			else if(i == 20)
			{
			    SendClientMessage(playerid, COLOR_WHITE, !"{33AA33}[Успех] {FFFFFF}Вы успешно прошли первый этап");
			    SendClientMessage(playerid, COLOR_WHITE, !"{FF8C00}[Выезжайте из учебной части и сдайте экзамен вождение по городу]");
			    DisablePlayerRaceCheckpoint(playerid);
				SetPlayerRaceCheckpoint(playerid,0,Race_auto[i][0],Race_auto[i][1],Race_auto[i][2],Race_auto[i][3],Race_auto[i][4],Race_auto[i][5],4.0);
			}
		}
		//метка автошкола сдал на права
		else if(race_a[playerid]==sizeof(Race_auto))
		{
		    race_a[playerid] = -1;
		    SetPVarInt(playerid,"auto_school_car_work", 0);
		    auto_school_car_key[playerid] = false;
		    auto_school_car_error[playerid] = 0;
		    DisablePlayerRaceCheckpoint(playerid);
			race_a[playerid] = -1;
            SendClientMessage(playerid, COLOR_WHITE, !"{33AA33}[Успех] {FFFFFF}Вы успешно сдали экзамен");
            PI[playerid][pCarLic] = YES_LICENSE;
            UpdatePlayerData(playerid, "CarLic", PI[playerid][pCarLic]);
            SendClientMessage(playerid, COLOR_GREEN, !"{33AA33}[Успех] {00FF00}Поздравляем, Вы получили водительские права");
			SendClientMessage(playerid, COLOR_WHITE, !"{FFC800}[Подсказка] {FFFFFF}Используйте {FFCC00}/lic [ID игрока]{FFFFFF}, для показа лицензий другому игроку");
			ExitCar(playerid);

			if(PI[playerid][pQuest] == 2 && PI[playerid][pQuestStatus] != 0 && PI[playerid][pQuestProcess] != 1)
			{
		        PI[playerid][pQuestProcess] = 1;
		        UpdatePlayerData(playerid, "QuestProcess", PI[playerid][pQuestProcess]);
