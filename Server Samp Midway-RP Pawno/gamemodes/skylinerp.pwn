//метка тюнинг сток покраски первичный
stock SetPainColorOne(playerid, color)
{
	if(CarInfof[TuningFamVehID[playerid]][cColor_1] == color) return SendClientMessage(playerid, -1, "{F70000}[Ошибка] {ADAEAD}Транспорт уже покрашен в этот цвет");
	if(PI[playerid][pMoney] < 5000) return SendClientMessage(playerid, -1, "{F70000}[Ошибка] {ADAEAD}У тебя не достаточно денег. Нужно{5BD93B} 5000$");
	GameTextForPlayer(playerid, "~r~-5000$", 5000, 1);
	PI[playerid][pMoney] -= 5000; PlayerPlaySound(playerid, 1133, 0.0, 0.0, 0.0);
	GiveMoney(playerid, -5000, "покраска авто семья");
	SendClientMessage(playerid,COLOR_WHITE,"{32CD32}[Успех] {FFFFFF}Вы покрасили свой транспорт");
	CarInfof[TuningFamVehID[playerid]][cColor_1] = color;
	VehicleInfo[TuningFamVehID[playerid]][vColor_1] = color;
	new color1, color2;
	GetVehicleColor(TuningFamVehID[playerid], color1, color2);
	
	ChangeVehicleColor(TuningFamVehID[playerid], VehicleInfo[TuningFamVehID[playerid]][vColor_1], color2);
	UpdateFamVehData("cColor1", CarInfof[TuningFamVehID[playerid]][cColor_1], TuningFamVehID[playerid]);
	TuneDialog(playerid);
	return 1;
}

