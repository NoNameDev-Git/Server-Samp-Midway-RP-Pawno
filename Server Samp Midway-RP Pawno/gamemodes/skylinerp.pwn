//метка семья сток продажи машины
stock SaleAutoFam(playerid, id)
{
    new query_delete_fcar[50];
	new mes[24];
	format(query_delete_fcar, sizeof query_delete_fcar, "DELETE FROM `familycars` WHERE `ID` = '%d'", FamSaleAuto[playerid][id][sfID]);
	mysql_tquery(mysql, query_delete_fcar);

	new vehicleid = FamSaleAuto[playerid][id][sfVehID];
	RemoveVehicleNeon(vehicleid);
	DestroyVehicle(vehicleid);

	LoadFamilyCarBuy();

	new countfcar = 0;
    for(new i; i < OWNABLECARSF; i++)
	{
	    if(CarInfof[i][cFamID] == PI[playerid][pFamily])
	    {
	        countfcar ++;
	        UpdateFamCarData("cNum", countfcar, CarInfof[i][cID]);
		}
	}

	LoadFamilyCarBuy();

	GiveMoney(playerid, (FamSaleAuto[playerid][id][sfSalePrice]-floatround(0.2*FamSaleAuto[playerid][id][sfSalePrice])), "продажа авто семья");
    format(mes,sizeof(mes),"~g~+ %d$",(FamSaleAuto[playerid][id][sfSalePrice]-floatround(0.2*FamSaleAuto[playerid][id][sfSalePrice])));
	GameTextForPlayer(playerid, mes, 3000, 1);

	SendClientMessage(playerid, COLOR_WHITE, !"{7AC71F}[Успешно] {FFFFFF}Вы продали семейный транспорт");
}

