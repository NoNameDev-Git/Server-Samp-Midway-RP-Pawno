//метка дом установить спавн личного транспорта
CMD:setpos(playerid)
{
//метка авторынок
    if(!PI[playerid][pCars]) return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}У Вас нет личного т/с");

    new vehicle, vehicleid;
    for(new j; j < MAX_VEHICLES; j++)
    {
        if(CarInfo[j][cID] == PI[playerid][pCars])
        {
            vehicleid = CarInfo[j][cVehID];
		}
	}
    
	vehicle = GetVehID(vehicleid);
	for(new i; i < MAX_AUTOMARKET; i++)
	{
		if(vehicle == AutoMarketInfo[i][aCar_ID])
		{
	      	SendClientMessage(playerid, COLOR_GREY, "Вы не можете установить позицию т/с так как он в продаже на авторынке");
			RemovePlayerFromVehicle(playerid);
			HideCarInfo(playerid);
			ClearAnimations(playerid);
			return 1;
		}
	}


    new PlayerN[MAX_PLAYERS];
   	GetPlayerName(playerid, PlayerN[playerid], MAX_PLAYER_NAME);
    for(new i; i < OWNABLEHOUSES; i++)
	{
	    if(HouseInfo[i][hOwner] == PlayerN[playerid])
	    {
  		    new Float:x, Float:y, Float:z, Float:r;
  		    GetPlayerPos(playerid, x, y, z);
  		    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
  		    {
				GetPlayerFacingAngle(playerid,r);
			}
  		    else
  		    {
  		        new currentveh;
  		        currentveh = GetPlayerVehicleID(playerid);
  		        GetVehicleZAngle(currentveh, r);
	  		}
  		    HouseInfo[i][hPark_X] = x;
  		    HouseInfo[i][hPark_Y] = y;
  		    HouseInfo[i][hPark_Z] = z;
  		    HouseInfo[i][hPark_R] = r;

  		    new request[2513];
			format(request, sizeof(request), "UPDATE `houses` SET `Park_X` = '%f', `Park_Y` = '%f', `Park_Z` = '%f', `Park_R` = '%f' WHERE `Owner` = '%s'",
		    HouseInfo[i][hPark_X], HouseInfo[i][hPark_Y], HouseInfo[i][hPark_Z], HouseInfo[i][hPark_R], PlayerN[playerid]);
			mysql_query(mysql, request);
            return SendClientMessage(playerid, COLOR_GREY, !"Вы установили спавн личного т/с");
	    }
 	}
 	return 1;
}


