stock UpdateAutoMarketPark(playerid,inputtext,NMarket,NPark)
{
	new formstr[300];
	format(formstr, sizeof(formstr), "{32CD32}[Успех] {FFFFFF}Вы приобрели парковочное место №%d на авторынке",NPark);
	SendClientMessage(playerid, COLOR_GREEN, formstr);
    SetPlayerChatBubble(playerid, "{FF0000}- 7000$", COLOR_RED, 10.0, 5000);
	GameTextForPlayer(playerid, "~r~- 7000$", 3000, 1);

	AutoMarketInfo[NMarket][aCost] = inputtext;
	AutoMarketInfo[NMarket][aOwner_ID] = PI[playerid][pID];
	AutoMarketInfo[NMarket][aCar_ID] = PI[playerid][pCars];
	AutoMarketInfo[NMarket][aActive] = 1;
	AutoMarketInfo[NMarket][aDays] = 2;

	for(new j; j < MAX_HOUSES; j++)
	{
		if(HouseInfo[j][hID] == PI[playerid][pHouseKey])
		{
            AutoMarketInfo[NMarket][ahPark_X] = HouseInfo[j][hPark_X];
            AutoMarketInfo[NMarket][ahPark_Y] = HouseInfo[j][hPark_Y];
            AutoMarketInfo[NMarket][ahPark_Z] = HouseInfo[j][hPark_Z];
            AutoMarketInfo[NMarket][ahPark_R] = HouseInfo[j][hPark_R];
            AutoMarketInfo[NMarket][aHome] = HouseInfo[j][hID];
		}
	}
	
	new request[2513];
	format(request, sizeof(request), "UPDATE `automarket` SET `ID_Car` = '%d', `Owner_ID` = '%d', `Cost` = '%d', `Active` = '%d', `hPark_X` = '%f', `hPark_Y` = '%f', `hPark_Z` = '%f', `hPark_R` = '%f', `Days` = '%d', `Home` = '%d' WHERE `ID` = '%d'",
    AutoMarketInfo[NMarket][aCar_ID],
	AutoMarketInfo[NMarket][aOwner_ID],
	AutoMarketInfo[NMarket][aCost],
	AutoMarketInfo[NMarket][aActive],
	AutoMarketInfo[NMarket][ahPark_X],
	AutoMarketInfo[NMarket][ahPark_Y],
	AutoMarketInfo[NMarket][ahPark_Z],
	AutoMarketInfo[NMarket][ahPark_R],
	AutoMarketInfo[NMarket][aDays],
	AutoMarketInfo[NMarket][aHome],
	AutoMarketInfo[NMarket][aID]);
	mysql_query(mysql, request);

    for(new k; k < MAX_VEHICLES; k++)
    {
		if(CarInfo[k][cID] == AutoMarketInfo[NMarket][aCar_ID])
		{
			AutoMarketInfo[NMarket][aMilage] = CarInfo[k][cMilage];
			AutoMarketInfo[NMarket][aModel] = CarInfo[k][cModelID] + 1;
		}
	}

	for(new a; a < 220; a++)
    {
		if(Transport[a][trID] == AutoMarketInfo[NMarket][aModel])
		{
		    strcat(AutoMarketInfo[NMarket][aName],Transport[a][trName]);
		    AutoMarketInfo[NMarket][aTank] = Transport[a][trTank];
		}
	}



	new formattext[500];
	format(formattext, sizeof(formattext), "Место №%d\n\
	{FFFFFF}Владелец: {1E90FF}%s\n\
	{FFFFFF}Модель: {FF8C00}%s\n\
	{FFFFFF}Пробег: {FF8C00}%.1f ml\n\
	{FFFFFF}Объем топливного бака: {FF8C00}%d lt\n\
	{FFFFFF}Стоимость: {32CD32}%d$",
	NPark,
	PN(playerid),
	AutoMarketInfo[NMarket][aName],
	AutoMarketInfo[NMarket][aMilage],
	AutoMarketInfo[NMarket][aTank],
	AutoMarketInfo[NMarket][aCost]);
	UpdateDynamic3DTextLabelText(AutoMarketText[NMarket], 0xFF1493FF, formattext);

    new house;
	for(new i; i < MAX_HOUSES; i++)
	{
		if(PI[playerid][pHouseKey] == HouseInfo[i][hID])
		{
			HouseInfo[i][hPark_X] = AutoMarketInfo[NMarket][aPark_X];
			HouseInfo[i][hPark_Y] = AutoMarketInfo[NMarket][aPark_Y];
			HouseInfo[i][hPark_Z] = AutoMarketInfo[NMarket][aPark_Z];
			HouseInfo[i][hPark_R] = AutoMarketInfo[NMarket][aPark_R];
			house = i;
		}
	}

	format(request, sizeof(request), "UPDATE `houses` SET `Park_X` = '%f', `Park_Y` = '%f', `Park_Z` = '%f', `Park_R` = '%f' WHERE `Owner` = '%s'",
    AutoMarketInfo[NMarket][aPark_X],
	AutoMarketInfo[NMarket][aPark_Y],
	AutoMarketInfo[NMarket][aPark_Z],
	AutoMarketInfo[NMarket][aPark_R],
	PN(playerid));
	mysql_query(mysql, request);

	if(IsPlayerInAnyVehicle(playerid))RemovePlayerFromVehicle(playerid);
	else ClearAnimations(playerid);
	HideCarInfo(playerid);

	J_SetPlayerFacingAngle(playerid, 179.4537);
	SetPlayerPos(playerid, 342.1877,-1791.2273,4.9103);

	new slot = GetCarSlot(PI[playerid][pCars]),
		query_save_house[381],
	 	modelid = CarInfo[slot][cModelID];

	format(query_save_house, sizeof query_save_house, "UPDATE `cars` SET `cFuel` = '%.2f', `cMilage` = '%.2f' WHERE `cOwnerID` = '%d'", CarInfo[slot][cFuel], CarInfo[slot][cMilage], PI[playerid][pID]);
	mysql_tquery(mysql, query_save_house);

	new carID = PI[playerid][pCars];
	DestroyVehicle(CarInfo[GetCarSlot(carID)][cVehID]);

	new vehicleid = CreateVehicle(Transport[modelid][trModel], HouseInfo[house][hPark_X], HouseInfo[house][hPark_Y], HouseInfo[house][hPark_Z], HouseInfo[house][hPark_R], CarInfo[slot][cColor_1], CarInfo[slot][cColor_2], -1);
	CarInfo[slot][cVehID] = vehicleid;

	if(CarInfo[slot][cHealth] < 300.0)
		CarInfo[slot][cHealth] = 1000.0;

	J_SetVehicleHealth(vehicleid, CarInfo[slot][cHealth]);
	ToggleLock(vehicleid);
	VehicleInfo[vehicleid][vFuel] = CarInfo[slot][cFuel];
	VehicleInfo[vehicleid][vMillage] = CarInfo[slot][cMilage];
	VehicleInfo[vehicleid][vHealth] = CarInfo[slot][cHealth];
	VehicleInfo[vehicleid][vOwnerid] = CarInfo[slot][cOwnerID];
	SetVehicleNumberPlate(vehicleid, CarInfo[slot][cNumber]);
	SetDoorsStatus(vehicleid, 1);

	for(new tn; tn < 14; tn++)
		if(CarInfo[slot][cTuning][tn] != 0) AddVehicleComponent(vehicleid, CarInfo[slot][cTuning][tn]);

	ChangeVehiclePaintjob(vehicleid, CarInfo[slot][cPaintJob]);

	foreach(new i : Player) if(IsPlayerConnected(i) && IsVehicleStreamedIn(vehicleid,i)) SetVehicleParamsForPlayer(vehicleid,i,0,1);
	VehicleInfo[vehicleid][vLock] = 1;
    SetDoorsStatus(vehicleid, 1);
}






















