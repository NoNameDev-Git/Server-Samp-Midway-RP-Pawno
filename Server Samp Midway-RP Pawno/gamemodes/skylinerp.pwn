		HouseInfo[i][hMoney] = cache_get_row_int(i, 20, mysql);
		HouseInfo[i][hDrugs] = cache_get_row_int(i, 21, mysql);
		HouseInfo[i][hMats] = cache_get_row_int(i, 22, mysql);
