public SetEngineStatus(vehicleid)
{
	new engine, lights, alarm, doors, bonnet, boot, objective;
    GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
	if(engine <= 0)
	{
	    new arend_id = INVALID_PLAYER_ID;
     	if(GetArendCarID(vehicleid, arend_id) != (-1))
       		if(arend_id == INVALID_PLAYER_ID || !IsPlayerConnected(arend_id))
			   return 0;
		
		new modelid = GetVehicleModel(vehicleid) - 400;
		
		if(modelid < 0)
			return 1;

		if(Transport[modelid][trFuelable] == 1)
		{
			if(VehicleInfo[vehicleid][vFuel] <= 0.0 || VehicleInfo[vehicleid][vHealth] <= 300)
				return -1;
		}
		
		else
			engine = 1;

		engine = 1;
	}
	
	else if(engine > 0)
		engine = 0;
		
    SetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
	return engine;
}
