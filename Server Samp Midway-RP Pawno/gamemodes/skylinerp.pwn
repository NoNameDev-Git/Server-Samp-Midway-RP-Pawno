public OnPlayerEnterCheckpoint(playerid)
{
	 //метка грузчики выгружаем груз и получаем деньги
	if(MoversTwo[playerid])
	{
	    if(IsPlayerInRangeOfPoint( playerid, 10.0, -155.8994,-289.5796,3.9053))
		{
		    DisablePlayerCheckpoint(playerid);
		    ClearAnimations(playerid);
			RemovePlayerAttachedObject(playerid, 3);
			ApplyAnimation(playerid, !"CARRY", !"putdwn", 1.0, 0, 1, 1, 0, 0, 1);
			SetPlayerSpecialAction(playerid, 0);
			//создаём пикап где возьмём груз
			MoversMoney[playerid] += 220;
            MoversOne[playerid] = SetPlayerCheckpoint(playerid,-95.5272,-210.1267,1.4219, 3);
			new string[79];
			format(string, sizeof(string), "Вы положили ящик. {42B02C}+$220{FFFFFF} к зарплате. Заработано: {FF4500}$%d", MoversMoney[playerid]);
			SendClientMessage(playerid, COLOR_RED, string);
			SetPVarInt(playerid,"MoversWorkGruz", 0);
			//пустые руки
			//проверка на окончание рабочего дня
			new Movershour,Moversminute,Moverssecond;
			gettime(Movershour,Moversminute,Moverssecond);
			if(Movershour == 14 && Moversminute >= 20)
			{
	   			new stringtimeo[110];
				format(stringtimeo, sizeof(stringtimeo), "Обеденный перерыв в 14:30 и до 15:30 {FFFFFF}Время: %d:%d Успейте сдать форму, иначе вам не выплатят зарплату", Movershour, Moversminute);
				SendClientMessage(playerid, COLOR_RED, stringtimeo);
			}
			if(Movershour == 19 && Moversminute >= 50)
			{
				new stringtime[100];
				format(stringtime, sizeof(stringtime), "Конец рабочего дня в 20:00 {FFFFFF}Время: %d:%d Успейте сдать форму, иначе вам не выплатят зарплату", Movershour, Moversminute);
				SendClientMessage(playerid, COLOR_RED, stringtime);
			}
			if(Movershour == 14 && Moversminute >= 30)
			{
			    MoversMoney[playerid] = 0;
			    DisablePlayerCheckpoint(playerid);
			    ClearAnimations(playerid);
				RemovePlayerAttachedObject(playerid, 3);
