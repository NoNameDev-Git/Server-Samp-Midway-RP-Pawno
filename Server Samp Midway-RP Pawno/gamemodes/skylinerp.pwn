	        case 'K': word[i] = 'k';
	        case 'L': word[i] = 'l';
	        case 'M': word[i] = 'm';
	        case 'N': word[i] = 'n';
	        case 'O': word[i] = 'o';
	        case 'P': word[i] = 'p';
	        case 'Q': word[i] = 'q';
	        case 'R': word[i] = 'r';
	        case 'S': word[i] = 's';
	        case 'T': word[i] = 't';
	        case 'U': word[i] = 'u';
