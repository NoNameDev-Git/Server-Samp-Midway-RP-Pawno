						AutoMarketInfo[i][aModel] = CarInfo[j][cModelID] + 1;
					}
				}
			    
			    for(new a; a < 220; a++)
			    {
					if(Transport[a][trID] == AutoMarketInfo[i][aModel])
					{
					    strcat(AutoMarketInfo[i][aName],Transport[a][trName]);
