	}
    new slot;
    switch(type)
    {
        case 1..11,19..21,25:slot=0;
        case 12,26: slot=1;
        case 15: slot=2;
        case 13,22,24: slot=3;
		case 14: slot=4;
		case 16..18,23,27: slot=5; //28 - елка без сохранения
		default: slot=-1;
    }
	if(slot != -1)
		PI[playerid][pSlotItem][slot] = setobject;

	return true;
}


