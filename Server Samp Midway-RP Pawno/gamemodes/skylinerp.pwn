				format(totalstring, 256, "[AC] Игрок %s[%d] был кикнут. Код №1: %d | Код №2: %d (Other)", PN(playerid), playerid, code, code2);
				SendAdminsMessage(COLOR_LIGHTRED, totalstring);
			}
		}
	}
	return true;
