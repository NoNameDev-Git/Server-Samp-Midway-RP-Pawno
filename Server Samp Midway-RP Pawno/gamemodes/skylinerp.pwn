forward OnPlayerLoaded(playerid);
public OnPlayerLoaded(playerid)
{
	new
		rows, fields;

 	cache_get_data(rows, fields, mysql);
    
    if(rows)
	{
	    PI[playerid][pID] = cache_get_field_content_int(0, "ID");
	    cache_get_field_content(0, "Password", PI[playerid][pPassword], mysql, 65);
	    cache_get_field_content(0, "Code", PI[playerid][pCode], mysql, 25);
	    cache_get_field_content(0, "LastIP", PI[playerid][pLastIP], mysql, 16);
	    cache_get_field_content(0, "GoogleAuth", PI[playerid][pGoogleAuth], mysql, 18);
	    PI[playerid][pGoogleAuthCheck] = cache_get_field_content_int(0, "GoogleAuthCheck");
	    PI[playerid][pGoogleAuthStatus] = cache_get_field_content_int(0, "GoogleAuthStatus");
		new query[79 + 9];
		format(query, sizeof query, "SELECT `time`, `admin`, `reason` FROM `ban` WHERE `takeid` = '%d' AND `status` = '1'", PI[playerid][pID]);
		mysql_function_query(mysql, query, true, "CheckBan", "dd", playerid, PI[playerid][pID]);
		//ShowLoginDialog(playerid);
	}
	else
	{
	    RegisterState[playerid] = 1;
	   	ShowRegisterDialog(playerid,RegisterState[playerid]);
	}
}
forward CheckBan(playerid, id);
public CheckBan(playerid, id)
{
	new rows, field;
	cache_get_data(rows, field);
	if(rows)
	{
	    new
	        time,
			admin,
			nickname[MAX_PLAYER_NAME],
			reason[70];
			
		time = cache_get_row_int(0, 0);
		admin = cache_get_row_int(0, 1);
		cache_get_row(0, 2, reason, mysql);
		GetPlayerNameByID(admin, nickname);

		if(time <= 0)
		{
		    new
		        query_delete_ban[50];
		        
		    format(query_delete_ban, sizeof query_delete_ban, "DELETE FROM `ban` WHERE `takeid` = '%d'", id);
		    mysql_tquery(mysql, query_delete_ban);
		    ShowLoginDialog(playerid);
		}
		else
		{
		    new
		        frmt_ban_message[146];
		        
			format(frmt_ban_message, sizeof frmt_ban_message, "Вы забанены по причине: '%s' администратором %s. Осталось дней: %d", reason, nickname, time);
			SendClientMessage(playerid, 0xFF182DFF, frmt_ban_message);
			J_Kick(playerid);
		}
	}
	else
	    ShowLoginDialog(playerid);
	    
	return true;
}


