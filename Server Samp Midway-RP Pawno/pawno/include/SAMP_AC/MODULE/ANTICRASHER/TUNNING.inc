static IsAValidTuningModelForVehicle(modelid, componentid)
{
	if(modelid > 0)
	{
		switch(componentid)
		{
			case 1000..1191:
			{
				componentid -= 1000;
				if(GUARD_MODEL_TUNING[(modelid - 400) * 6 + (componentid >>> 5)] & 1 << (componentid & 0b00011111)) return 1;
			}
			case 1192, 1193:
			{
				if(modelid == 576) return 1;
			}
		}
	}
	return 0;
}
public OnVehicleMod(playerid, vehicleid, componentid)
{
	if(IsAValidTuningModelForVehicle(GetVehicleModel(vehicleid), componentid) == 0)
	{
		SAMP_AC_SEND_WARNING_FOR_SCRIPT(playerid, "Tunning Crasher", 93, 0);
	}
#if defined SAMP_AC_OnVehicleMod
    return SAMP_AC_OnVehicleMod(playerid, vehicleid, componentid);
#endif
}
